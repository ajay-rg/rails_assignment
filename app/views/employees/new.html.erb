<h1> Employee Details</h1>
<%= form_for :employee, url: employees_path do |f| %>
  <p>
    <%= f.label :Name %><br>
    <%= f.text_area :Name %>
  </p>
  <p>
    <%=f.label :Project %><br>
    <%= f.select(:Project, options_for_select([['Tribe Local'], ['Report Garden'],['Ad Word Kits']]), { include_blank: true }) %>
  </p>
  <p>
    <%= f.label :Team_lead %><br>
    <%= f.text_area :Team_lead %>
  </p>
  <p>
    <%= f.label :Skill_number %><br>
    <%= f.text_area :Skill_number %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
